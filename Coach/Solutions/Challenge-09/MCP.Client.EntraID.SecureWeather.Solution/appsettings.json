{
  "AzureOpenAI": {
    "Endpoint": "your_model_endpoint",
    "ApiKey": "your_azure_openai_api_key_here",
    "DeploymentName": "your_model_deployment _name i.e gpt-4"
  },
  "AzureAD": {
    "ClientId": "your_azure_ad_app_registration_client_id",
    "ClientSecret": "your_azure_ad_app_registration_client_secret",
    "TenantId": "your_azure_ad_tenant_id_or_common_for_multitenant"
  },
  "MCPServer": {
    "UseLocalMCP": "false",
    "RemoteMCP": {
      "Endpoint": "your_mcp_server_endpoint"
    },
    "LocalMCP": {
      "Name": "WeatherMcpServer",
      "Command": "dotnet",
      "Arguments": [ "run", "--project", "../WeatherMcpServer/WeatherMcpServer.csproj" ]
    }
  }
}
