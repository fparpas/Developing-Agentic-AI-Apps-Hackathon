{
  "AzureOpenAI": {
    "Endpoint": "your_model_endpoint",
    "ApiKey": "your_azure_openai_api_key_here",
    "DeploymentName": "your_model_deployment _name i.e gpt-4"
  },
  "MCPServer": {
    "UseLocalMCP": "true",
    "RemoteMCP": {
      "Endpoint": "your_mcp_server_endpoint"
    },
    "LocalMCP": {
      "Name": "WeatherMcpServer",
      "Command": "dotnet",
      "Arguments": [ "run", "--project", "../WeatherMcpServer/WeatherMcpServer.csproj" ]
    }
  }
}
